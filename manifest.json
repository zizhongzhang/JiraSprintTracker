{
  "name": "Getting Started Example",
  "version": "1.0",
  "description": "Build an Extension!",
  "permissions": ["storage"],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "manifest_version": 2,
  "content_scripts": [
    {
      "matches": ["https://{jira_domain}/*"],
      "js": ["myscript.js"]
    }
  ]
}