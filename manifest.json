{
    "name": "Jira Sprint Tracker",
    "version": "1.0",
    "description": "Tell you how much you are ahead/behind in current sprint",
    "permissions": ["storage"],
    "manifest_version": 2,
    "content_scripts": [
        {
            "matches": ["https://*/*"],
            "js": ["myscript.js"]
        }
    ],
    "page_action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "images/sprint_tracker16.png",
            "32": "images/sprint_tracker32.png",
            "48": "images/sprint_tracker48.png",
            "128": "images/sprint_tracker128.png"
        }
    },
    "icons": {
        "16": "images/sprint_tracker16.png",
        "32": "images/sprint_tracker32.png",
        "48": "images/sprint_tracker48.png",
        "128": "images/sprint_tracker128.png"
    },
    "options_page": "options.html"
}
